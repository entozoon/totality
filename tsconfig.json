{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2017", "es7", "es6", "dom"],
    "declaration": false,
    "outDir": "./dist",
    "removeComments": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
